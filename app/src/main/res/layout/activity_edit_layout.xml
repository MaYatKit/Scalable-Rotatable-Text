<?xml version="1.0" encoding="utf-8"?>
<com.example.sticker.view.RuptChildClickRelativeLayout
    android:id="@+id/activity_edit_parent_layout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >



    <FrameLayout
        android:id="@+id/video_edit_text_fragment"
        android:layout_width="match_parent"
        android:layout_height="@dimen/video_edit_bottom_height2"
        android:layout_alignParentBottom="true"
        tools:layout="@layout/fragment_video_edit_text" />

    <FrameLayout
        android:id="@+id/activity_edit_center_fragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout="@layout/fragment_edit_text"
        android:layout_above="@id/video_edit_text_fragment"/>





</com.example.sticker.view.RuptChildClickRelativeLayout>